[run]
source = mlx_websockets
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */_version.py
    */htmlcov/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @(abc\.)?abstractmethod
    except ImportError
    pass

[html]
directory = htmlcov
